<element extends="ide\scripts\elements\AbstractComponent">
    <init>

    </init>
    <properties>
        <property code="url" name="URL" editor="text" tooltip="URL для подключения к WebSocket" />
        <property code="connectionTimeout" name="Таймаут соединения" editor="integer" tooltip="Максимальное время для подключения" value="15000" />
        <property code="headers" name="Загаловки для подключения" editor="text" value="15000" />
    </properties>

    <properties group="proxy" title="Прокси" sort="2000">
        <property code="proxyHost" name="Хост" editor="text" />
        <property code="proxyPort" name="Порт" editor="integer" value="80" />
        <property code="proxyPassword" name="Пароль" editor="text" />
        <property code="proxySecure" name="Безопасный прокси" editor="boolean" />
    </properties>

    <eventTypes>
        <eventType code="connected" name="Открытие" kind="ObjectEvent" icon="icons/ok16.png" />
        <eventType code="connectError" name="Ошибка подключения" kind="ObjectEvent" icon="icons/error16.png" />
        <eventType code="textMessage" name="Текстовое сообщение" kind="ObjectEvent" icon="icons/chat16.png" />
        <eventType code="frame" name="Фрейм" kind="ObjectEvent" icon="icons/chat16.png" />
        <eventType code="stateChanged" name="Изменение состояния" kind="ObjectEvent" icon="icons/chat16.png" />
        <eventType code="disconnected" name="Закрытие" kind="ObjectEvent" icon="icons/bulletRed16.png" />
    </eventTypes>
</element>